type User {
  id: ID!
  username: String!
  email: String!
  fullname: String
  birthday: Time
  maxAge: Int
  role: String!
  createdAt: Time!
  updatedAt: Time!
}

type DailyCoins {
  remainingCoins: Int!
  savedCoins: Int!
  wastedCoins: Int!
  activeTask: Task
}

extend type Query {
  getUserInfo: User!
  getDailyCoins: DailyCoins
}
